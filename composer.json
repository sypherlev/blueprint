{
  "name": "sypherlev/blueprint",
  "description": "DBAL package that performs data operations through applying patterns",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "sql",
    "database",
    "query"
  ],
  "autoload": {
    "psr-4": {
      "SypherLev\\Blueprint\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests"
    }
  },
  "prefer-stable": true,
  "require": {
    "php": "~7.1",
    "ext-pdo": "*"
  },
  "require-dev": {
    "codeception/codeception": "^4.1",
    "codeception/module-phpbrowser": "^1.0.0",
    "codeception/module-asserts": "^1.0.0",
    "codeception/module-rest": "^1.2",
    "codeception/module-db": "^1.0",
    "vimeo/psalm": "^3.12"
  },
  "authors": [
    {
      "name": "Claire Ryan",
      "email": "claire@raynfall.com"
    }
  ]
}